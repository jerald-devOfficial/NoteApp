{
  "name": "server",
  "version": "1.0.0",
  "description": "Note app",
  "main": "index.js",
  "engines": {
    "node": "15.2.1",
    "npm": "6.14.10"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js --ignore './client/'",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "npm i --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jerald-devOfficial/NoteApp.git"
  },
  "author": "Jerald Baroro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jerald-devOfficial/NoteApp/issues"
  },
  "homepage": "https://github.com/jerald-devOfficial/NoteApp#readme",
  "dependencies": {
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.6"
  }
}
